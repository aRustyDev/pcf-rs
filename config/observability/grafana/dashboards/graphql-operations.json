{
  "dashboard": {
    "id": null,
    "title": "GraphQL Operations",
    "tags": ["pcf", "graphql", "operations"],
    "style": "dark",
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "GraphQL Request Rate by Operation",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(graphql_request_total[5m])) by (operation)",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Requests/sec",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "GraphQL Request Duration by Operation",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(graphql_request_duration_seconds_bucket[5m])) by (operation, le))",
            "legendFormat": "P95 - {{operation}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(graphql_request_duration_seconds_bucket[5m])) by (operation, le))",
            "legendFormat": "P50 - {{operation}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Duration",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "GraphQL Errors by Operation",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(graphql_request_total{status=\"error\"}[5m])) by (operation)",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "Errors/sec",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 4,
        "title": "Operation Cardinality Monitor",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (operation)(graphql_request_total))",
            "legendFormat": "Unique Operations",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 40
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(graphql_request_total, operation)",
          "hide": 0,
          "includeAll": true,
          "label": "Operation",
          "multi": true,
          "name": "operation",
          "options": [],
          "query": {
            "query": "label_values(graphql_request_total, operation)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "sort": 1,
          "type": "query"
        }
      ]
    }
  }
}